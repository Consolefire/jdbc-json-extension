<?xml version="1.0"?>
<!DOCTYPE suppressions PUBLIC
    "-//Checkstyle//DTD SuppressionFilter Configuration 1.2//EN"
    "https://checkstyle.org/dtds/suppressions_1_2.dtd">

<suppressions>
	<suppress files=".*[\\/]src[\\/](test|it)[\\/]"
		checks="JavadocPackage" />
	<suppress files=".*[\\/]src[\\/](test|it)[\\/]"
		checks="JavadocMethod" />
	<suppress files="[\\/]generated-src[\\/]"
		checks="[a-zA-Z0-9]*" />
	<suppress files="[\\/]test[\\/]" checks="MagicNumber*" />
	<suppress files="[\\/]test[\\/]" checks="MethodCount*" />
	<suppress files="[\\/]test[\\/]" checks="MethodLength*" />
	<suppress files="[\\/]integration-test[\\/]"
		checks="MagicNumber*" />
	<suppress files="[\\/]integration-test[\\/]"
		checks="MethodCount*" />
</suppressions>